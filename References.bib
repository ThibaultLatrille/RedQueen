issn = {0014-3820},
journal = {Evolution; international journal of organic evolution},
keywords = {Animals,Host-Parasite Interactions,Host-Parasite Interactions: physiology,Linear Models,Moths,Moths: parasitology,Reproduction,Reproduction: physiology,Rhabditida,Rhabditida: microbiology,Rhabditida: physiology,Symbiosis,Symbiosis: physiology,Xenorhabdus,Xenorhabdus: physiology},
month = apr,
number = {4},
pages = {932--42},
pmid = {18194474},
title = {{A survival-reproduction trade-off in entomopathogenic nematodes mediated by their bacterial symbionts.}},
