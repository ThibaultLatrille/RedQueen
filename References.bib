author = {Lambert, Amaury},
doi = {10.1016/j.tpb.2006.01.002},
file = {:D$\backslash$:/Dropbox/Mendeley/Lambert - 2006 - Probability of fixation under weak selection a branching process unifying approach.pdf:pdf},
issn = {0040-5809},
journal = {Theoretical population biology},
keywords = {Alleles,Animals,Evolution, Molecular,Genetic Linkage,Genetic Speciation,Genetics, Population,Genetics, Population: statistics \& numerical data,Logistic Models,Models, Genetic,Mutation,Population Density,Population Dynamics,Probability,Selection, Genetic,Stochastic Processes},
month = jun,
number = {4},
pages = {419--41},
pmid = {16504230},
