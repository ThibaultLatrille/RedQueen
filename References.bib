issn = {0932-0113},
journal = {Parasitology research},
keywords = {Animals,Host-Parasite Interactions,Phylogeny,Rhabditida,Rhabditida: classification,Rhabditida: microbiology,Symbiosis,Xenorhabdus,Xenorhabdus: isolation \& purification,Xenorhabdus: pathogenicity},
month = dec,
number = {6},
pages = {520--4},
pmid = {14557877},
title = {{Effect of native \textit{Xenorhabdus} on the fitness of their \textit{Steinernema} hosts: contrasting types of interaction.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/14557877},
volume = {91},
year = {2003}
}
@article{Somvanshi2010,
abstract = {Summary Fimbriae are adhesive organelles known to enable pathogens to colonize animal tissue, but little is known of their function in mutualistic symbioses. Photorhabdus colonization of Heterorhabditis bacteriophora nematodes is essential for the pair's insect pathogenic lifestyle. Maternal nematodes acquire Photorhabdus symbionts as a persistent intestinal biofilm prior to transmission to infective juvenile (IJ) stage offspring developing inside the maternal body. Screening 8000 Photorhabdus mutants for defects in IJ colonization revealed that a single fimbrial locus, named mad for maternal adhesion defective, is essential. The mad genes encode a novel usher/chaperone assembled fimbria regulated by an ON/OFF invertible promoter switch. Adherent Photorhabdus cells in maternal nematode intestines had the switch ON opposite to the OFF orientation of most other cells. A DeltamadA mutant failed to adhere to maternal intestines and be transmitted to the IJs. Mad fimbriae were detected on TT01 phase ON cells but not on DeltamadA phase ON cells. Also required for transmission is madJ, predicted to encode a transcriptional activator related to GrlA. Expression of madA-K or madIJK restored the ability of madJ mutant to adhere. The Mad fimbriae were not required for insect pathogenesis, indicating the specialized function of Mad fimbriae for symbiosis.},
author = {Somvanshi, Vishal S and Kaufmann-Daszczuk, Bettina and Kim, Kwi-Suk and Mallon, Shane and Ciche, Todd A.},
doi = {10.1111/j.1365-2958.2010.07270.x},
file = {:D$\backslash$:/Dropbox/Mendeley/Somvanshi et al. - 2010 - Photorhabdus phase variants express a novel fimbrial locus, mad, essential for symbiosis.pdf:pdf},
issn = {1365-2958},
journal = {Molecular microbiology},
keywords = {07270,10,1021,1038,1111,1365-2958,2010,4,77,a novel fimbrial locus,doi,ecular microbiology,first published online 6,j,july 2010,photorhabdus phase variants express,x},
month = jul,
number = {July},
pages = {1021--1038},
pmid = {20572934},
title = {{\textit{Photorhabdus} phase variants express a novel fimbrial locus, mad, essential for symbiosis.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/20572934},
volume = {77},
year = {2010}
}
@article{Somvanshi2012,
abstract = {Microbial populations stochastically generate variants with strikingly different properties, such as virulence or avirulence and antibiotic tolerance or sensitivity. Photorhabdus luminescens bacteria have a variable life history in which they alternate between pathogens to a wide variety of insects and mutualists to their specific host nematodes. Here, we show that the P. luminescens pathogenic variant (P form) switches to a smaller-cell variant (M form) to initiate mutualism in host nematode intestines. A stochastic promoter inversion causes the switch between the two distinct forms. M-form cells are much smaller (one-seventh the volume), slower growing, and less bioluminescent than P-form cells; they are also avirulent and produce fewer secondary metabolites. Observations of form switching by individual cells in nematodes revealed that the M form persisted in maternal nematode intestines, were the first cells to colonize infective juvenile (IJ) offspring, and then switched to P form in the IJ intestine, which armed these nematodes for the next cycle of insect infection.},
author = {Somvanshi, Vishal S and Sloup, Rudolph E and Crawford, Jason M and Martin, Alexander R and Heidt, Anthony J and Kim, Kwi-suk and Clardy, Jon and Ciche, Todd A.},
doi = {10.1126/science.1216641},
file = {:D$\backslash$:/Dropbox/Mendeley/Somvanshi et al. - 2012 - A single promoter inversion switches Photorhabdus between pathogenic and mutualistic states.pdf:pdf},
issn = {1095-9203},
journal = {Science},
keywords = {Animals,Fimbriae Proteins,Fimbriae Proteins: genetics,Gene Expression Regulation, Bacterial,Genome, Bacterial,Intestines,Intestines: microbiology,Moths,Moths: microbiology,Mutation,Phenotype,Photorhabdus,Photorhabdus: cytology,Photorhabdus: genetics,Photorhabdus: growth \& development,Photorhabdus: pathogenicity,Promoter Regions, Genetic,Rhabditoidea,Rhabditoidea: microbiology,Sequence Inversion,Symbiosis,Virulence,Virulence: genetics},
month = jul,
number = {6090},
pages = {88--93},
pmid = {22767929},
title = {{A single promoter inversion switches \textit{Photorhabdus} between pathogenic and mutualistic states.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/22767929},
volume = {337},
year = {2012}
}
